<?xml version="1.0" encoding="UTF-8"?>
<eventFormatter name="HealthStatPublishEventFormatter" statistics="disable" trace="disable" xmlns="http://wso2.org/carbon/eventformatter">
    <from streamName="health_statistics_stats" version="1.0.0"/>
    <mapping customMapping="enable" type="map">
        <property>
            <from name="ID"/>
            <to name="ID"/>
        </property>
        <property>
            <from name="timeStamp"/>
            <to name="timeStamp"/>
        </property>
        <property>
            <from name="value"/>
            <to name="value"/>
        </property>
        <property>
            <from name="type"/>
            <to name="type"/>
        </property>
    </mapping>
    <to eventAdaptorName="OutPutadaptorRDBMStoPublishHealthStat" eventAdaptorType="mysql">
        <property name="database.name">DataSourcetoPublishHealthStatRDBMS</property>
        <property name="table.name">HealthStatistics</property>
    </to>
</eventFormatter>