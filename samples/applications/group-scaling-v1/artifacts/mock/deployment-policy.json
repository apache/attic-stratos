{
    "applicationId": "group-scaling-v1",
    "applicationPolicy": {
        "networkPartition": [
            {
                "id": "network-partition-1",
                "activeByDefault": "true",
                "partitions": [
                    {
                        "id": "partition-1",
                        "provider": "mock",
                        "property": [
                            {
                                "name": "region",
                                "value": "RegionOne"
                            }
                        ]
                    }
                ]
            },
            {
                "id": "network-partition-2",
                "activeByDefault": "false",
                "partitions": [
                    {
                        "id": "partition-2",
                        "provider": "mock",
                        "property": [
                            {
                                "name": "region",
                                "value": "RegionOne"
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "childPolicies": [
        {
            "alias": "mygroup2",
            "networkPartition": [
                {
                    "id": "network-partition-1",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-1",
                            "max": 2
                        }
                    ]
                },
                {
                    "id": "network-partition-2",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-2",
                            "max": 2
                        }
                    ]
                }
            ]
        }
    ]
}

