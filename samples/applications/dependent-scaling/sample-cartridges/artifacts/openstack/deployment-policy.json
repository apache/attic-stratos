{
    "applicationId": "sample-cartridges-app",
    "applicationPolicy": {
        "applicationId": "sample-cartridges-app",
        "networkPartition": [
            {
                "id": "network-partition-1",
                "activeByDefault": "true",
                "partitions": [
                    {
                        "id": "partition-1",
                        "provider": "openstack",
                        "property": [
                            {
                                "name": "region",
                                "value": "RegionOne"
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "childPolicies": [
        {
            "alias": "mytomcat",
            "networkPartition": [
                {
                    "id": "network-partition-1",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-1",
                            "max": 5
                        }
                    ]
                }
            ]
        },
        {
            "alias": "myphp",
            "networkPartition": [
                {
                    "id": "network-partition-1",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-1",
                            "max": 10
                        }
                    ]
                }
            ]
        }
    ]
}
