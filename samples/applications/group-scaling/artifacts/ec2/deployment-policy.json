{
    "applicationPolicy": {
        "networkPartition": [
            {
                "id": "network-partition-1",
                "activeByDefault": "true",
                "partitions": [
                    {
                        "id": "partition-1",
                        "provider": "ec2",
                        "property": [
                            {
                                "name": "region",
                                "value": "ap-southeast-1"
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "childPolicies": [
        {
            "alias": "myc4",
            "networkPartition": [
                {
                    "id": "network-partition-1",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-1",
                            "max": 2
                        }
                    ]
                }
            ]
        },
        {
            "alias": "group1c1",
            "networkPartition": [
                {
                    "id": "network-partition-1",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-1",
                            "max": 4
                        }
                    ]
                }
            ]
        },
        {
            "alias": "mygroup2",
            "networkPartition": [
                {
                    "id": "network-partition-1",
                    "partitionAlgo": "one-after-another",
                    "partitions": [
                        {
                            "id": "partition-1",
                            "max": 2
                        }
                    ]
                }
            ]
        }
    ]
}

